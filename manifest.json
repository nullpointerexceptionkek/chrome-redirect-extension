{
    "manifest_version": 3,
    "name": "Website Redirector",
    "version": "1.0",
    "permissions": ["storage", "declarativeNetRequest", "declarativeNetRequestFeedback"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["*://*/*"]
}
